{
  "extends": [
    "tslint-config-standard",
    "tslint-config-prettier"
  ],
  "rules": {
    "member-ordering": [
      true,
      {
        "order": "instance-sandwich",
        "alphabetize": true
      }
    ],
    "no-empty": [true, "allow-empty-functions"],
    "ter-no-sparse-arrays": false,
    "ordered-imports": true
  }
}
