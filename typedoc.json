{
  "name": "Reactive-JS",
  "entryPoints": "./packages/core",
  "entryPointStrategy": "expand",
  "exclude": [
    "./packages/core/__internal__/**/**/*",
    "./packages/core/__tests__/**/**/*",
    "./packages/core/**/__internal__/**/*"
  ],
  "readme": "none",
  "out": "docs",
  "excludeExternals": true,
  "excludePrivate": true,
  "excludeProtected": true,
  "disableSources": true,
  "markedOptions": {
    "mangle": false
  },
  "tsconfig": "tsconfig.typedoc.json"
}
