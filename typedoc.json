{
  "name": "Reactive-JS",
  "entryPoints": ["./packages/core"],
  "entryPointStrategy": "expand",
  "exclude": [
    "./packages/core/__internal__/**/**/*",
    "./packages/core/__tests__/**/**/*",
    "./packages/core/**/__classes__/**/*",
    "./packages/core/**/__internal__.*",
    "./packages/core/**/__internal__/**/*",
    "./packages/core/**/__mixins__/**/*",
    "./packages/core/**/__private__.*",
    "./packages/core/**/__private__/**/*"
  ],
  "readme": "none",
  "out": "docs",
  "excludeExternals": true,
  "excludePrivate": true,
  "excludeProtected": true,
  "disableSources": true,
  "markedOptions": {
    "mangle": false
  },
  "tsconfig": "tsconfig.typedoc.json"
}
