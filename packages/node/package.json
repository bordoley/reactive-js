{
  "name": "@reactive-js/node",
  "version": "0.0.37",
  "keywords": [
    "asynchronous",
    "data flow",
    "data-flow",
    "frp",
    "functional programming",
    "functional-reactive-programming",
    "ix",
    "observable",
    "observables",
    "react",
    "reactjs",
    "reactive",
    "reactive-extensions",
    "reactive-js",
    "reactive-x",
    "rx",
    "rxjs",
    "scheduler",
    "state management",
    "state-management",
    "stream",
    "streams",
    "web"
  ],
  "author": {
    "name": "David Bordoley",
    "email": "bordoley@gmail.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/bordoley/reactive-js.git"
  },
  "dependencies": {
    "@reactive-js/core": "^0.0.37",
    "iconv-lite": "^0.5.1",
    "mime-db": "^1.43.0"
  },
  "devDependencies": {
    "@types/jest": "^24.0.23",
    "@types/mime-db": "^1.27.1",
    "@types/node":"13.13.2",
    "jest": "^24.9.0",
    "renamer": "^2.0.0",
    "ts-jest": "^24.1.0"
  },
  "scripts": {
    "build": "yarn build-cjs && yarn build-esm && yarn build-mjs && yarn docs",
    "build-cjs": "tsc -p tsconfig.cjs.json",
    "build-esm": "tsc -p tsconfig.esm.json",
    "build-mjs": "rm -rf ./dist/mjs && cp -r ./dist/esm ./dist/mjs && renamer --find \".js\" --replace \".mjs\" ./dist/mjs/** ./dist/mjs/**/**/** && cp  -r ./dist/cjs/ ./dist/mjs",
    "clean": "rm -rf dist && rm -rf docs",
    "docs": "node ./scripts/docs.mjs",
    "test": "jest",
    "test-coverage": "jest --coverage",
    "test-debug": "node --inspect-brk ./node_modules/.bin/jest",
    "typecheck": "tsc -p tsconfig.typecheck.json"
  },
  "jest": {
    "preset": "ts-jest",
    "globals": {
      "ts-jest": {
        "tsConfig": "./tsconfig.esm.json"
      }
    }
  },
  "publishConfig": {
    "access": "public"
  }
}
