{
  "name": "reactive-js",
  "private": true,
  "scripts": {
    "build": "rm -rf ./build ./mod && tsc -p tsconfig.compile.json && rollup -c rollup.config.js",
    "bootstrap": "lerna bootstrap",
    "deno-install": "curl -fsSL https://deno.land/x/install/install.sh | sh",
    "format": "prettier '**' --write",
    "lint": "eslint . --fix -f codeFrame --ext .ts,.tsx,.js,.jsx",
    "release": "lerna publish --force-publish",
    "test": "jest",
    "test-coverage": "jest --coverage",
    "test-debug": "node --inspect-brk ./node_modules/.bin/jest",
    "test-deno": "deno test -r ./scripts/testRunner.deno.ts"
  },
  "devDependencies": {
    "@most/core": "^1.6.1 ",
    "@most/scheduler": "^1.3.0 ",
    "@rollup/plugin-typescript": "^6.1.0",
    "@types/benchmark": "^2.1.0",
    "@types/jest": "^26.0.15",
    "@types/node": "^14.14.9",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/scheduler": "^0.16.1",
    "@typescript-eslint/eslint-plugin": "^4.8.1",
    "@typescript-eslint/parser": "^4.8.1",
    "benchmark": "^2.1.4",
    "coveralls": "^3.1.0",
    "csstype": "^3.0.5",
    "eslint": "^7.14.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.22.1",
    "jest": "^26.6.3",
    "lerna": "^3.22.1",
    "prettier": "^2.2.0",
    "react": "^17.0.1",
    "rollup": "^2.33.3",
    "rollup-plugin-dts": "^2.0.0",
    "rxjs": "^6.6.3",
    "scheduler": "^0.20.1",
    "svelte": "^3.29.7",
    "ts-jest": "^26.4.4",
    "typedoc": "0.20.0-beta.8",
    "typedoc-plugin-markdown": "^3.0.11",
    "typedoc-plugin-no-inherit": "^1.2.0",
    "typescript": "^4.1.2",
    "wonka": "^4.0.15"
  },
  "jest": {
    "preset": "ts-jest",
    "testRegex": "testRunner.jest.ts",
    "globals": {
      "ts-jest": {
        "tsConfig": "./tsconfig.compile.json"
      }
    }
  },
  "workspaces": [
    "packages/*"
  ]
}
