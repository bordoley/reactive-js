{
  "name": "reactive-js",
  "private": true,
  "scripts": {
    "build": "tsc -p tsconfig.compile.json && rm -rf ./build && rollup -c rollup.config.js",
    "bootstrap": "lerna bootstrap",
    "deno-install": "curl -fsSL https://deno.land/x/install/install.sh | sh",
    "format": "prettier '**' --write",
    "lint": "eslint . --fix -f codeFrame --ext .ts,.tsx,.js,.jsx",
    "release": "lerna publish --force-publish",
    "test": "jest",
    "test-coverage": "jest --coverage",
    "test-debug": "node --inspect-brk ./node_modules/.bin/jest",
    "test-deno": "deno test -r ./scripts/testRunner.deno.ts"
  },
  "devDependencies": {
    "@most/core": "^1.5.0",
    "@most/scheduler": "^1.2.3",
    "@rollup/plugin-typescript": "^6.1.0",
    "@types/benchmark": "^1.0.33",
    "@types/jest": "^26.0.3",
    "@types/node": "14.0.14",
    "@types/react": "^16.9.41",
    "@types/scheduler": "^0.16.1",
    "@typescript-eslint/eslint-plugin": "^3.5.0",
    "@typescript-eslint/parser": "^3.5.0",
    "benchmark": "^2.1.4",
    "coveralls": "^3.1.0",
    "eslint": "^7.4.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-import": "^2.22.0",
    "jest": "^26.1.0",
    "lerna": "^3.22.1",
    "prettier": "^2.0.5",
    "react": "^16.11.0",
    "renamer": "^2.0.1",
    "replace": "^1.2.0",
    "rollup-plugin-dts": "^2.0.0",
    "rxjs": "^6.6.0",
    "scheduler": "^0.19.1",
    "svelte": "^3.23.2",
    "ts-jest": "^26.1.1",
    "typedoc": "0.17.0-3",
    "typedoc-plugin-markdown": "^2.3.1",
    "typedoc-plugin-no-inherit": "^1.1.10",
    "typescript": "^3.9.6",
    "wonka": "^4.0.14"
  },
  "jest": {
    "preset": "ts-jest",
    "testRegex": "testRunner.jest.ts",
    "globals": {
      "ts-jest": {
        "tsConfig": "./tsconfig.esm.json"
      }
    }
  },
  "workspaces": [
    "packages/*"
  ]
}
